@article{Dalby92,
	Abstract = {Motivation is in training conductors to judge the
	intonation of an ensemble. They need to be able to locate and identify
	errors among a number of voices.  Training in interval and chord
	recognition skills may not be relevant.  (Gives some refs)  Their
	system included the use of musical passages in multiple voices, which
	previous similar systems didn't.  The training program using the
	computer was shown to improve students' abilities significantly.
	Students' attitudes were mostly positive.  Exercises used longer
	examples and also isolated parts of them.  Students had 3 types of
	exercise:  listening (students select mistunings and listen to them,
	and compare with in-tune version), judging (students identify location
	and type of errors in examples) and tuning (students alter pitches to
	tune examples)
Student listens to example any number of times, and can also listen to
	an in-tune version.  Then indicates through multiple choice what the
	problem is.  Yes/No feedback immediately.  Can try again if wrong.  If
	wrong too many times, right answer is displayed.  Also had a 'quiz'
	(testing) mode in which in-tune references were not available.
Difficulty levels correspond to level of mistuning, controlled
	automatically by system for good performance on a quiz (student can
	choose when to take quiz)
Had similar effects on different ability levels.
Low correlation between harmonic intonation discrimination skill and
	harmonic analysis skill.},
	Author = {Dalby, Bruce},
	Date-Added = {2017-03-18 10:53:13 +0000},
	Date-Modified = {2017-03-18 10:53:14 +0000},
	Journal = {Journal of Research in Music Education},
	Library = {own copy},
	Number = {2},
	Pages = {139--152},
	Title = {A Computer-Based Training Program for Developing Harmonic Intonation Discrimination Skill},
	Volume = {40},
	Year = {1992}}

