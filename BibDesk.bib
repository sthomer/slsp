@mastersthesis{Pearce00,
	Abstract = {The objective of this research was to design a creative
 aid to musical composition: a system that would generate a (user
 specified) number of drum patterns, within a specified style and
 showing a sufficient amount of variation, on any one run. To achieve
 these aims a genetic algorithm was implemented using as its critic a
 multi--layer perceptron, trained on a set of {"drum&bass"}
 patterns. Domain specifiic knowledge was incorporated into a number
 of areas of the GA and an island model was used to generate multiple
 solutions. Experiments conducted, using human subjects, questioned
 the aesthetic merit and style of the generated patterns. These
 partial failures of the system to acheive the stated aims were
 attributed to shortcomings of the data used to train the ANN
 critic. Suggestions for future research have been presented in terms
 of improving the critic, extending the approach to more complicated
 rhythmic sequences and better evaluation of machine compositions.},
	Address = {UK},
	Author = {Pearce, M. T.},
	Date-Added = {2017-03-18 10:53:13 +0000},
	Date-Modified = {2017-03-18 10:53:16 +0000},
	School = {Department of Artificial Intelligence, University of Edinburgh},
	Title = {Generating Rhythmic Patterns: {A} Combined Neural and Evolutionary Approach},
