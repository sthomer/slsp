@incollection{Hargreaves96,
	Abstract = {In music, {"there is no common body of developmental theory
		which guides pedagogical practice"}p49.  Gives a
		similar quote from Keith Swanwick, who says that music
		education research hasn't assimilated results from
		general educational thought and practice.(1977)
	    Little point in designing teaching methods without any
		underlying rationale.  Research should be theory
		driven.  Cognitive, social and affective aspects of
		development must be considered together.
	    Children take an active part in their own development.
	    Cognitive psychology - anything that emphasises internal
		rules, processes and states.
            Development of melodic perception: global to analytic
		strategies. Movement from gross to precise features.
	    Often explanations are in terms of schemas (including
		Piagetian theories).  Niesser (1976) described them as
		{"cognitive structures that prepare the perciever to
		accept certain kinds of information rather than
		others."}
	    Cognitive-developmental psychology - most applicable to
		music education.  Largely applications of Piaget's
		theory. Doubts about this, because time-dependency of
		music makes it difficult to produce musical analogues
		of Piaget's materials, in which objects for comparison
		were presented simultaneously.
	    Various theories of musical appreciation: immature to
		mature (more formal), pre-symbolic and symbolic.
	    Behavioural psychology - easily applied to teaching and
		much used in the States: competency-based approach.
		Effective in the teaching of lower level performance
		skills which can easily be specified, but not good for
		higher-order skills like composition.
	    Formal and intuitive aspects of music - former often
		emphasised at the expense of the latter.
},
	Address = {London},
	Author = {Hargreaves, David},
	Booktitle = {Teaching Music},
	Chapter = {3},
	Date-Added = {2017-03-18 10:53:13 +0000},
	Date-Modified = {2017-03-18 10:53:15 +0000},
	Editor = {Spruce, Gary},
	Library = {Moray House},
	Pages = {49--62},
	Publisher = {Routledge},
	Title = {The developmental psychology of music: Scope and aims},
	Year = {1996}}

@inproceedings{HenzLauerEtAl96,
	Abstract = {The goal of this work is to derive four--voice music
 pieces from given musical plans, which describe the harmonic flow and
 the intentions of a desired composition.  We developed the
 experimentation platform COMPOzE for intention--based
 composition. COMPOzE is based on constraint programming over finite
 domains of integers. We argue that constraint programming provides a
 suitable technology for this task and that the libraries and tools
 available for the constraint programming system Oz effectively
 support the implementation of COMPOzE. This work links the research
 areas of of automatic music composition on one hand and finite domain
 constraint programming on the other, and contributes the tool
 COMPOzE, which practically demonstrates the potential of constraint
 programming to open up new areas of application for automatic music
 composition.},
