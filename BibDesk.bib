@article{Todd91,
	Abstract = {With the advent of von Neumann--style computers, music
 composers can now develop algorithms embodying the compositional
 rules they are interested in and then use a computer to carry out
 these algorithms. The arrival of a new paradigm for computing has
 made a different approach to algorithmic composition possible. This
 new computing paradigm is called parallel distributed processing
 (PDP), also known as connectionism. The author presents a particular
 type of PDP network for music composition applications. Various
 issues are discussed in designing the network, choosing the music
 representation used, training the network, and using it for
 composition. Comparisons are made to previous methods of algorithmic
 composition, and examples of the network's output are presented. The
 author provides an indication of the power and range of PDP methods
 for algorithmic composition in order to encourage others to begin
 exploring this approach. Hence, rather than merely presenting a
 reduced compositional technique, alternative approaches and
 tangential ideas are included throughout as points of departure for
 further efforts.},
	Author = {Todd, P. M.},
	Date-Added = {2017-03-18 10:53:13 +0000},
	Date-Modified = {2017-03-18 10:53:14 +0000},
	Journal = {Computer Music Journal},
	Note = {Also in Music and Connectionism, 1991, P.M. Todd and D.G. Loy (Eds), MIT Press},
	Number = {4},
	Pages = {27--43},
	Title = {A connectionist approach to algorithmic composition},
	Url = {http://www--abc.mpib--berlin.mpg.de/users/ptodd/publications/todd11/todd11.htm},
	Volume = {13},
	Year = {1991},
	Bdsk-Url-1 = {http://www--abc.mpib--berlin.mpg.de/users/ptodd/publications/todd11/todd11.htm}}

@inproceedings{Todd91a,
	Abstract = {A new approach to computer art applications is provided by
 the rapidly expanding field of neural networks. Rather than operate
 in the traditional style of preprogrammed rule--following systems,
 neural networks have the power to *learn* to produce specific types
 of outputs from specific inputs, based on examples they are
 taught. Thus, instead of having to specify *how* to create a certain
 artwork, the artist can instead teach a network *examples* of the
 desired output, and have the network generate new instances in that
 style. We need not specify the steps involved in creating a Rodin
 sculpture -- we can just collect instances of the sorts of sculpture
 we'd like to get and use those to train a network. This sort of thing
 is the promise of neural network applications in the arts, stated in
 a very provocative and exaggerated manner. But we are beginning to
 realize this promise in limited domains, and the space for further
 applications and explorations of these techniques is wide open. In
 this paper, I will briefly describe neural networks, some of what
 they can and can't do, some of what they have done already in the
 arts, primarily music, and some of what they can be applied to in
 other areas.},
	Address = {Philadelphia, PA},
	Author = {Todd, P. M.},
